@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(AstroComm REQUIRED COMPONENTS Core)
find_dependency(Threads REQUIRED)

# Conditionally find Boost if available
find_dependency(Boost QUIET COMPONENTS system thread)

# Conditionally find OpenSSL if available
find_dependency(OpenSSL QUIET)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/AstroCommDeviceTargets.cmake")

check_required_components(AstroCommDevice)
